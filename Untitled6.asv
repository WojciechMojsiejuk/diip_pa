I = im2double(imread('measurement_2_.JPG'));
I = rgb2gray(I);
I = imcomplement(I);
imshow(I)
[Gmag, Gdir] = imgradient(I,'intermediate');
se = strel('disk',30);
BW = imclose(Gmag,se);
% BW = imopen(BW,se);
h = fspecial('disk',20);
BW = imfilter(BW,h,'replicate');
BW = imgaussfilt(BW,10);
BW = (BW>0.05);
imshowpair(I,BW,'montage'), hold on
[centers, radii]=imfindcircles(BW, [190,320],'Sensitivity',0.97)
viscircles(centers, radii,'EdgeColor','b','LineWidth',1);